{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "React Boilerplate",
  "scripts": {
    "lint": "eslint --config=.eslintrc ./src/components/**/*.jsx ./src/**/*.js ./src/*.js --fix",
    "build": "npm-run-all --parallel client-build server-build",
    "server-build": "NODE_ENV='development' webpack --config ./webpack/webpack.server.config.js",
    "client-build": "NODE_ENV='development' webpack --config ./webpack/webpack.browser.config.js --progress --colors --watch",
    "build-prod": "npm-run-all client-build-prod server-build-prod",
    "server-build-prod": "NODE_ENV='production' webpack --config ./webpack/webpack.server.config.js -p",
    "client-build-prod": "NODE_ENV='production' webpack --config ./webpack/webpack.browser.config.js -p",
    "test": "jest",
    "test-update": "jest -u",
    "test-coverage": "jest --coverage --verbose",
    "test-dev": "jest --watch",
    "start": "NODE_ENV=development node dist/main.js"
  },
  "author": "Ateev",
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "compression": "1.7.2",
    "express": "4.16.3",
    "history": "^4.6.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "3.6.0",
    "superagent": "3.8.3",
    "webpack": "4.8.1"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-engine-plugin": "0.2.1",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "22.4.3",
    "jest-cli": "22.4.3",
    "jest-serializer-enzyme": "1.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.9.0",
    "npm-run-all": "4.1.3",
    "null-loader": "0.1.1",
    "react-addons-test-utils": "15.6.2",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "uglify-js": "3.3.24",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack-cli": "2.1.3",
    "webpack-dev-middleware": "3.1.3",
    "webpack-dev-server": "3.1.4",
    "webpack-hot-middleware": "2.22.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/src/mocks/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg|png)$": "<rootDir>/src/mocks/fileMock.js",
      "config.js": "<rootDir>/config/config.js.sample"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-enzyme"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.jsx",
      "src/**/*.js",
      "src/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "src/mocks",
      "src/vendors",
      "src/static"
    ]
  }
}
